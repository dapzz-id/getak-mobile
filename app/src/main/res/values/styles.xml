<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PrefixTextSmall" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="PrefixText1240" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="PrefixText600Land" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="PrefixText600_800Port" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="PrefixText800Land" parent="TextAppearance.Material3.BodySmall">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="HeaderCell">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">120dp</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">#FF5C75</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="InputBoxStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>